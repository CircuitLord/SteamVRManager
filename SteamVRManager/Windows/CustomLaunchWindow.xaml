<Window x:Class="SteamVRManager.Windows.CustomLaunchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SteamVRManager.Windows"
        mc:Ignorable="d"
        Title="Custom Launch Programs" Height="221" Width="401">
    <Grid>
        <ListBox Name="UIProgramList" HorizontalAlignment="Left" Height="140" Margin="10,10,0,0" VerticalAlignment="Top" Width="372"/>
        <Button Name="UINew" Content="New" HorizontalAlignment="Left" Margin="10,155,0,0" VerticalAlignment="Top" Width="75" Click="UINew_Click"/>
        <Button Name="UIRemove" Content="Remove" HorizontalAlignment="Left" Margin="90,155,0,0" VerticalAlignment="Top" Width="76" Click="UIRemove_Click"/>


    </Grid>
</Window>
